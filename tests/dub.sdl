name "stl-containers-tests"
description "Tests for STL containers"
authors "Manu"
copyright "Copyright Â© 2019, Manu"
license "proprietary"
dependency "stl-containers" path="../"
preGenerateCommands "clang++ -c cpp/string_cpp.cpp -obuild/string_cpp.o" platform="posix"
preGenerateCommands "clang++ -c cpp/vector_cpp.cpp -obuild/vector_cpp.o" platform="posix"
preGenerateCommands "clang++ -c cpp/string_cpp.cpp -obuild/string_cpp.obj" platform="windows"
preGenerateCommands "clang++ -c cpp/vector_cpp.cpp -obuild/vector_cpp.obj" platform="windows"
sourceFiles "source/string_test.d" "source/vector_test.d" "build/string_cpp.o" "build/vector_cpp.o" platform="posix"
sourceFiles "source/string_test.d" "source/vector_test.d" "build/string_cpp.obj" "build/vector_cpp.obj" platform="windows"
targetType "executable"
